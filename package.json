	"scripts": {
		"react:start": "craco start",
		"react:build": "craco build",
		"react:test": "craco test",
		"lint": "eslint src --ext js,jsx,ts,tsx",
		"lint:fix": "eslint src --ext js,jsx,ts,tsx --fix",
		"start": "concurrently \"cross-env BROWSER=none yarn react:start\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
		"build:prepare": "yarn react:build && tsc -p electron",
		"build:nsis": "yarn build:prepare && electron-builder --windows nsis",
		"build:appx": "yarn build:prepare && electron-builder --windows appx",
		"build:publish": "yarn build:prepare && electron-builder --publish always"
	},
